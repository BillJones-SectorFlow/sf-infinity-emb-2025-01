# RunPod API Configuration
RUNPOD_BASE_URL=https://api.runpod.ai/v2
RUNPOD_TIMEOUT=300
RUNPOD_MAX_RETRIES=3

# Endpoint Type Selection
# Set to 'true' to use native /runsync endpoints (recommended)
# Set to 'false' to use OpenAI-compatible endpoints (for testing)
USE_NATIVE_ENDPOINTS=true

# Model Configuration  
MODEL_NAMES=michaelfeil/mxbai-rerank-large-v2-seq
BATCH_SIZES=32;16
MAX_TOKENS=8192
BACKEND=torch
DTYPES=auto;float32

# Logging Configuration
LOG_LEVEL=INFO
DEBUG=false
